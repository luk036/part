MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
SUBTARGETS =	 \
		sub-vector1 \
		sub-vector2

first: all

all: Makefile $(SUBTARGETS)

vector1/$(MAKEFILE): 
	cd vector1 && $(QMAKE)  -o $(MAKEFILE)
sub-vector1: vector1/$(MAKEFILE) FORCE
	cd vector1 && $(MAKE) -f $(MAKEFILE)

vector2/$(MAKEFILE): 
	cd vector2 && $(QMAKE)  -o $(MAKEFILE)
sub-vector2: vector2/$(MAKEFILE) FORCE
	cd vector2 && $(MAKE) -f $(MAKEFILE)

Makefile: STLport.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile STLport.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile STLport.pro

all: $(SUBTARGETS)
qmake_all: vector1/$(MAKEFILE) vector2/$(MAKEFILE)
	 ( [ -d vector1 ] && cd vector1 ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d vector2 ] && cd vector2 ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uninstall install uiclean mocclean lexclean yaccclean: qmake_all FORCE
	 ( [ -d vector1 ] && cd vector1 ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d vector2 ] && cd vector2 ; $(MAKE) -f $(MAKEFILE) $@; ) || true
distclean: qmake_all FORCE
	 ( [ -d vector1 ] && cd vector1 ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d vector2 ] && cd vector2 ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

